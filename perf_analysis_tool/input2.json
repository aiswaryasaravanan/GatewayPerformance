{
	"duration" : 15,
	"sample_frequency" : 5,
    "output_directory" : "/root/perf-analysis/result",
    "auto_mode" : {
        "consecutive_threshold_exceed_limit" : 3,
        "window_size" : 2
    },
	"monitor" : {
		"cpu" : [
			{
				"name" : "all",
				"trigger" : 3.75
			},
			{
				"name" : "edged"
			},
			{
				"name" : "dbus-daemon"
			}
		],
		"commands" : [
			{
				"name" : "handoff",
				"trigger" : 750
			},
			{
				"name" : "edged -v"
			},
			{
				"name" : "top -H -n 1"
			}
		],
		"counters" : [
			{
				"name" : "dpdk_counters"
			},
			{
				"name" : "memb.mod_dns_data_buffer_t.tot_bytes"
			},
			{
				"name" : "memb.mod_vc_peer_qos_seg_stats_t.tot_bytes"
			},
			{
				"name" : "memb.mod_vc_link_cos_qos_t.tot_bytes"
			},
			{
				"name" : "memb.mod_vc_qos_calc_deqrate_t.tot_bytes"
			},
			{
				"name" : "memb.mod_vc_qos_calc_deqrate_slot_t.tot_bytes"
			}		
		]
	},
	"analysis" : [
		"cpu",
		"handoff_drops",
		"counters"
	],
	"diag" : {
		"perf" : {
			"record" : {
				"sleep" : 3,
				"frequency" : 999,
				"number_of_record" : 2,
				"delay_between_record" : 2
			},
			"sched" : {
				"sleep" : 3,
				"number_of_sched" : 2,
				"delay_between_sched" : 2
			},
			"latency" : [
				"Switches",
				"Average delay"
			],
			"stat" : {
				"sleep" : 0.01,
				"events" : [
					"dTLB-loads",
					"dTLB-load-misses",
					"dTLB-stores", 
					"dTLB-store-misses", 
					"iTLB-loads", 
					"iTLB-load-misses", 
					"LLC-loads", 
					"LLC-load-misses", 
					"LLC-stores", 
					"LLC-store-misses", 
					"LLC-prefetch-misses", 
					"page-faults", 
					"cache-misses", 
					"cache-references"
				]
			}
		}
	}
}
